      "name": "No Authorization",
      "item": [
        {
          "name": "Get all projects without authentication key",
          "event": [
            {
              "listen": "test",
              "script": {
                "id": "a9897e4f-2175-44c3-bd39-d24e30534151",
                "exec": [
                  "const Ajv = require('ajv');",
                  "const ajv = new Ajv({logger: console});",
                  "const schema = {",
                  "    \"properties\": {",
                  "        \"error\": {",
                  "            \"type\": \"object\",",
                  "            \"required\": [\"code\", \"message\"],",
                  "            \"maxProperties\": 2,",
                  "            \"properties\": {",
                  "                \"code\": {\"type\": \"integer\"},",
                  "                \"message\": {\"type\": \"string\"}",
                  "            }",
                  "        }",
                  "    }",
                  "}",
                  "",
                  "const resObj = pm.response.json();",
                  "",
                  "pm.test(\"Status code is 400.\", function () {",
                  "    pm.response.to.have.status(400);",
                  "});",
                  "",
                  "pm.test(\"The response is JSON.\", function() {",
                  "    pm.response.to.be.json;",
                  "});",
                  "",
                  "pm.test(\"The response JSON is in valid format.\", function() {",
                  "    pm.expect(ajv.validate(schema, resObj)).to.be.true;",
                  "});",
                  "",
                  "pm.test(\"Status code in response body is 400.\", function() {",
                  "    pm.expect(pm.response.code).to.equal(400);",
                  "});",
                  "",
                  "pm.test(\"Error message in response body is ok.\", function() {",
                  "    pm.expect(pm.response.text()).to.include(\"Access token cannot be empty\");",
                  "});"
                ],
                "type": "text/javascript"
              }
            },
